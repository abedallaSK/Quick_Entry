    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@id/navigation_home_foreman">

    <fragment
        android:id="@+id/navigation_home_foreman"
        android:name="com.example.startapplication.ui.home_foreman.HomeForemanFragment"
        android:label="@string/title_home"
